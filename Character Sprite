;Sprite Ram has to be A0
41F9 0091 0400		lea A0 910400
102C 0391			move.b ($391,A4),D1
D040				add.w D0,D0
B8FC 884E 			cmp A4
66## ####			bne
43F9 0039 6970 		lea $396970,A1
3431 0000           move.w (A1,D0.w), D2
D2C2	            adda.w D2, A1
3419				move.w (A1+),D2
20D9				move.l (A1+),(A0+)
20D9				move.l (A1+),(A0+)
51CA FFFA			dbra D2
